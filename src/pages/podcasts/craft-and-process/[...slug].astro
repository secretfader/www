---
import { getCollection } from "astro:content";
import { textToSlug } from "../../../utils/feed";
import { YouTube } from "astro-embed";

export async function getStaticPaths() {
 const episodes = await getCollection("craft-and-process");
  return episodes.map(e => ({
   params: { slug: textToSlug(e.data.title) },
   props: e,
 }))
}

const { data: { title, youtube: { id: youtube_id } } } = Astro.props
---
<YouTube id={youtube_id}/>
